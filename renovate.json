{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:best-practices",
		"customManagers:biomeVersions",
		"github>Omochice/renovate-config:deno"
	],
	"enabledManagers": [
		"custom.regex"
	],
	"customManagers": [
		{
			"customType": "regex",
			"managerFilePatterns": [
				"/(?:^|/)deno\\.jsonc?$/"
			],
			"matchStrings": [
				"\"[^\"]+\"\\s*:\\s*\"npm:(?<depName>@?[^@\"]+)@\\^?(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
			],
			"datasourceTemplate": "npm"
		}
	]
}
